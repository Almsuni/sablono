#!/usr/bin/env bash
case $1 in
    advanced)
        phantomjs test-resources/phantomjs/test.js test-resources/phantomjs/advanced.html
        ;;
    none)
        phantomjs test-resources/phantomjs/test.js test-resources/phantomjs/none.html
        ;;
    *)
        phantomjs test-resources/phantomjs/test.js test-resources/phantomjs/none.html
        phantomjs test-resources/phantomjs/test.js test-resources/phantomjs/advanced.html
        ;;
esac
